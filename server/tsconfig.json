{
  "compilerOptions": {
    "lib": ["es2024", "ESNext.Array", "ESNext.Collection", "ESNext.Iterator"],
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    
    "rootDir": "./app",
    "outDir": "./build",
    "noEmit": true,
    "allowImportingTsExtensions": true,
  },
  "include": ["app/**/*.ts", "app/image-ai/model.js"],
  "exclude": ["dist"]
}

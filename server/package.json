{
  "name": "uofteam-board-server",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsx --watch --env-file=.env ./app/index.ts",
    "start": "node ./build/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write"
  },
  "dependencies": {
    "@google/genai": "^1.8.0",
    "@neplex/vectorizer": "^0.0.5",
    "@sequelize/postgres": "^7.0.0-alpha.46",
    "canvas": "^3.1.2",
    "cookie": "^1.0.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "konva": "^9.3.22",
    "morgan": "^1.10.0",
    "query-string": "^9.2.1",
    "sequelize": "^6.37.3",
    "socket.io": "^4.8.1",
    "stripe": "^18.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.1",
    "eslint": "^9.28.0",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0"
  },
  "imports": {
    "#*": "./app/*"
  }
